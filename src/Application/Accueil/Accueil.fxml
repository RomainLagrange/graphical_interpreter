<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<HBox fx:id="hBoxMain" prefHeight="448.0" prefWidth="800" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.Accueil.Controller_Accueil">
   <children>
      <AnchorPane prefHeight="448.0" prefWidth="400" GridPane.rowIndex="1">
         <children>
            <Button fx:id="button_valider" layoutX="320.0" layoutY="400.0" mnemonicParsing="false" onAction="#nextWindow" text="Run" />
            <AnchorPane fx:id="ap" prefHeight="364.0" prefWidth="400" GridPane.rowIndex="1">
               <children>
                  <Label layoutX="31.0" layoutY="138.0" text="Red Color" />
                  <Label layoutX="30.0" layoutY="95.0" text="Orange Color" />
                  <Label layoutX="29.0" layoutY="54.0" text="Green Color" />
                  <TextField fx:id="min_vert" layoutX="160.0" layoutY="49.0" prefHeight="26.0" prefWidth="80.0">
            </TextField>
                  <TextField fx:id="min_orange" layoutX="160.0" layoutY="90.0" prefHeight="26.0" prefWidth="80.0">
            </TextField>
                  <TextField fx:id="min_rouge" layoutX="161.0" layoutY="133.0" prefHeight="26.0" prefWidth="80.0">
            </TextField>
                  <TextField fx:id="max_vert" layoutX="280.0" layoutY="49.0" prefHeight="26.0" prefWidth="80.0">
            </TextField>
                  <TextField fx:id="max_orange" layoutX="280.0" layoutY="90.0" prefHeight="26.0" prefWidth="80.0">
            </TextField>
                  <TextField fx:id="max_rouge" layoutX="280.0" layoutY="133.0" prefHeight="26.0" prefWidth="80.0">
            </TextField>
                  <Button fx:id="button_table" layoutX="31.0" layoutY="193.0" mnemonicParsing="false" onAction="#choisirFichier" prefHeight="26.0" prefWidth="109.0" text="Choose table" />
                  <Button fx:id="button_file" layoutX="323.0" layoutY="193.0" mnemonicParsing="false" onAction="#acceptTable" text="OK" />
                  <Label fx:id="file_path" layoutX="157.0" layoutY="193.0" prefHeight="26.0" prefWidth="169.0" />
                  <ScrollPane layoutX="31.0" layoutY="241.0" prefHeight="127.0" prefWidth="329.0">
                     <GridPane fx:id="gridGene" prefWidth="254.0">
                        <columnConstraints>
                           <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="170.0" />
                           <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="80.0" />
                        </columnConstraints>
                     </GridPane>
                  </ScrollPane>
                  <Label layoutX="30.0" layoutY="14.0" text="Mutation color limits">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
   </children>
</HBox>
